<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN" "http://www.tuckey.org/res/dtds/urlrewrite3.1.dtd">
<urlrewrite>
<!-- removing trailing slashes for up to links up to CboS/XXXX/XXXX/XXXX/XXXX/ -->
     <rule match-type="regex">  
      <note>Remove trailing slash</note>
      <from>^/([^/]*)/$</from>
      <to type="redirect">../$1</to>
     </rule>
     <rule match-type="regex">  
      <note>Remove trailing slash</note>
      <from>^/([^/]*)/([^/]*)/$</from>
      <to type="redirect">../../$1/$2</to>
     </rule> 
     <rule match-type="regex">  
      <note>Remove trailing slash</note>
      <from>^/([^/]*)/([^/]*)/([^/]*)/$</from>
      <to type="redirect">../../../$1/$2/$3</to>
    </rule>
    <rule match-type="regex">  
      <note>Remove trailing slash</note>
      <from>^/([^/]*)/([^/]*)/([^/]*)/([^/]*)/$</from>
      <to type="redirect">../../../../$1/$2/$3/$4</to>
    </rule>
</urlrewrite>